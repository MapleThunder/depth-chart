{
  "compilerOptions": {
    "incremental": true,
    "target": "es2016",
    // "lib": [],
    "jsx": "preserve",
    "module": "commonjs",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
